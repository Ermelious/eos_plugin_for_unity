{
    "source_to_dest": [

        {"src": "*.md",                             "dest": "" },
        {"src": "etc/PackageTemplate/package.json", "dest": "" },
        {"src": "etc/PackageTemplate/Editor.meta",  "dest": "" },
        {"src": "etc/PackageTemplate/Runtime.meta", "dest": "" },

        {"src": "etc/PlatformSpecificAssets/EOS/*", "dest": "PlatformSpecificAssets~/EOS/", "recursive": true },
        {"src": "docs/*",                           "dest": "Documentation~/",              "recursive": true },
        {"src": "tools/bin/*",                      "dest": "bin~/",                        "recursive": true },

        {"src": "Assets/EOS/*.png", "dest": "Runtime/EOS/"},
                             
        {"src": "Assets/Plugins/*",                                   "dest": "Runtime/"                          },
        {"src": "Assets/Plugins/Source/Core/*",                       "dest": "Runtime/Core/",  "recursive": true },
        {"src": "Assets/Plugins/Source/Editor/*",                     "dest": "Editor/",        "recursive": true },

        {"src": "Assets/Plugins/Windows/*.cs",                           "dest": "Runtime/Windows/"      },
